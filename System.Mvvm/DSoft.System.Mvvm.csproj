<Project Sdk="Microsoft.NET.Sdk">
  <PropertyGroup>
    <TargetFrameworks>netstandard2.0; net6.0;</TargetFrameworks>
    <PackageId>DSoft.System.Mvvm</PackageId>
    <Summary>Core MVVM library for .Net</Summary>
    <PackageTags>MVVM</PackageTags>
    <Title>System.Mvvm</Title>
    <Description>Powerful Cross-platform MVVM class library for .Net Core, Net 6.+, UWP, Xamarin.iOS, Xamarin.Mac, Xamarin.Android and .Net Framework</Description>
    <PackageReleaseNotes>MAUI Updates</PackageReleaseNotes>
    <AssemblyName>System.Mvvm</AssemblyName>
    <RootNamespace>System.Mvvm</RootNamespace>
	  <Product>$(AssemblyName) ($(TargetFramework))</Product>
    <AssemblyVersion>2.2.2102.142</AssemblyVersion>
    <Version>2.2.2102.142-prerelease</Version>
    <FileVersion>2.2.2102.142</FileVersion>
    <PackageReadmeFile>readme.md</PackageReadmeFile>
  </PropertyGroup>
  <ItemGroup>
    <None Include="readme.md" Pack="true" PackagePath="\" />
    <PackageReference Update="NETStandard.Library" Version="2.0.3" />
  </ItemGroup>
</Project>
